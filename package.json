{
  "name": "@myeasyfarm/vite-plugin-icons-spritesheet",
  "version": "4.0.0-beta.1",
  "type": "module",
  "description": "Vite plugin that generates a spritesheet and types out of your icons folder.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "icons",
    "spritesheet",
    "vite",
    "plugin",
    "generator",
    "react",
    "angular",
    "vue",
    "remix"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.build.json",
    "remix:dev": "npm run dev -w test-apps/remix-vite",
    "build:dev": "tsc --project tsconfig.build.json",
    "build:dev:watch": "tsc --project tsconfig.build.json --watch",
    "dev": "npm-run-all -s build:dev -p remix:dev build:dev:watch",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "format": "biome format --write .",
    "prepublishOnly": "npm run build",
    "tsc": "tsc",
    "validate": "npm run lint && npm run tsc",
    "prepare": "husky"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MyEasyFarm/vite-plugin-icons-spritesheet.git"
  },
  "bugs": {
    "url": "https://github.com/MyEasyFarm/vite-plugin-icons-spritesheet/issues"
  },
  "files": [
    "dist"
  ],
  "workspaces": [
    ".",
    "test-apps/*"
  ],
  "homepage": "https://github.com/MyEasyFarm/vite-plugin-icons-spritesheet#readme",
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@xmldom/xmldom": "^0.9.6",
    "chalk": "^5.6.2",
    "tinyexec": "^1.0.1"
  },
  "peerDependencies": {
    "vite": ">=5.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@types/node": "^24.7.2",
    "husky": "^9.1.7",
    "npm-run-all2": "^8.0.4",
    "typescript": "^5.9.3",
    "vite": "7.1.10"
  }
}
